Algoritmo "semnome"

Var

  distancia, gasto, Media : real


Inicio

  escreva("Qual a distancia percorrida? ")
  leia(distancia)
  escreva("Quantos litros de gasolina foram gastos? ")
  leia(gasto)
  
  Media <- distancia / gasto
  escreval("O gasto médio de combustivel foi: ", Media:6:3)


Fimalgoritmo